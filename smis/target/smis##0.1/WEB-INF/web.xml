<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SAD - School Management Information System (SMIS)</display-name>
  <welcome-file-list>
    <welcome-file>login.ap</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>15</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>Login</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>org.rca.smis.web.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login.ap</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CreateUserServlet</display-name>
    <servlet-name>CreateUserServlet</servlet-name>
    <servlet-class>org.rca.smis.web.CreateUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateUserServlet</servlet-name>
    <url-pattern>/CreateUserServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ListUser</display-name>
    <servlet-name>ListUser</servlet-name>
    <servlet-class>org.rca.smis.web.ListUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListUser</servlet-name>
    <url-pattern>/listuser.ap</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CreateOrganization</display-name>
    <servlet-name>CreateOrganization</servlet-name>
    <servlet-class>org.rca.smis.web.CreateOrganization</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateOrganization</servlet-name>
    <url-pattern>/createorganization.ap</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ListOrganization</display-name>
    <servlet-name>ListOrganization</servlet-name>
    <servlet-class>org.rca.smis.web.ListOrganization</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListOrganization</servlet-name>
    <url-pattern>/listorganization.ap</url-pattern>
  </servlet-mapping>
</web-app>